{
    "compilerOptions": {
      "target": "ES6",
      "lib": ["dom", "es2015"],
      "types": ["cypress", "node"],
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": false,
      "noEmit": true
    },
    "include": ["**/*.ts"]
  }
  